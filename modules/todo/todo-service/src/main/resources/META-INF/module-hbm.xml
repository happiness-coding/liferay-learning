<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping auto-import="false" default-lazy="false">
    <class name="com.example.todo.model.impl.TodoItemImpl" table="Todo_TodoItem">
        <id name="todoItemId" type="long">
            <generator class="assigned" />
        </id>
        <property access="org.hibernate.property.BasicPropertyAccessor" name="uuid" type="com.liferay.portal.dao.orm.hibernate.StringType" />
        <property access="org.hibernate.property.BasicPropertyAccessor" name="groupId" type="com.liferay.portal.dao.orm.hibernate.LongType" />
        <property access="org.hibernate.property.BasicPropertyAccessor" name="companyId" type="com.liferay.portal.dao.orm.hibernate.LongType" />
        <property access="org.hibernate.property.BasicPropertyAccessor" name="userId" type="com.liferay.portal.dao.orm.hibernate.LongType" />
        <property access="org.hibernate.property.BasicPropertyAccessor" name="userName" type="com.liferay.portal.dao.orm.hibernate.StringType" />
        <property access="org.hibernate.property.BasicPropertyAccessor" name="createDate" type="org.hibernate.type.TimestampType" />
        <property access="org.hibernate.property.BasicPropertyAccessor" name="modifiedDate" type="org.hibernate.type.TimestampType" />
        <property access="org.hibernate.property.BasicPropertyAccessor" name="title" type="com.liferay.portal.dao.orm.hibernate.StringType" />
        <property access="org.hibernate.property.BasicPropertyAccessor" name="description" type="com.liferay.portal.dao.orm.hibernate.StringType" />
        <property access="org.hibernate.property.BasicPropertyAccessor" name="dueDate" type="org.hibernate.type.TimestampType" />
        <property access="org.hibernate.property.BasicPropertyAccessor" name="completed" type="com.liferay.portal.dao.orm.hibernate.BooleanType" />
        <property access="org.hibernate.property.BasicPropertyAccessor" name="priority" type="com.liferay.portal.dao.orm.hibernate.IntegerType" />
    </class>
</hibernate-mapping>